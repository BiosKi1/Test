<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Spectator" table="Spectator">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastname" type="string" column="lastname" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="firstname" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="age" type="integer" column="age" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="title" type="string" column="title" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
